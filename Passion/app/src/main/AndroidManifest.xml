<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.passion">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".src.ApplicationClass"
        android:allowBackup="false"
        android:fullBackupContent="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        tools:ignore="GoogleAppIndexingWarning"
        tools:targetApi="n">
        <activity android:name=".src.SignUp.SignUpActivity.SignUpActivity"></activity>
        <activity android:name=".src.SignUp.Email.SignupEmailActivity" />
        <activity android:name=".src.SignIn.FindPW.FindPwStartActivity" />
        <activity android:name=".src.TimerFragment.FragmentHome.statistics.AddSubjectActivity" />
        <activity android:name=".src.TimerFragment.FragmentStartActivity.FragmentStartActivity" />
        <activity android:name=".src.SignIn.SingInStart.SignInStartActivity" />
        <activity android:name=".src.StartActivity.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>